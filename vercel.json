{
  "functions": {
    "vercel_app.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/frontend/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel_app.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    }
  }
}